@article{bertawilde14,
  abstract      = {The conditional quantum mutual information {\$}I(A;B|C){\$} of a tripartite state {\$}\backslashrho{\_}{\{}ABC{\}}{\$} is an information quantity which lies at the center of many problems in quantum information theory. Three of its main properties are that it is non-negative for any tripartite state, that it decreases under local operations applied to systems {\$}A{\$} and {\$}B{\$}, and that it obeys the duality relation {\$}I(A;B|C) = I(A;B|D){\$} for a four-party pure state on systems {\$}ABCD{\$}. The conditional mutual information also underlies the squashed entanglement, an entanglement measure that satisfies all of the axioms desired for an entanglement measure. As such, it has been an open question to find Renyi generalizations of the conditional mutual information, that would allow for a deeper understanding of the original quantity and find applications beyond the traditional memoryless setting of quantum information theory. The present paper addresses this question, by defining different {\$}\backslashalpha{\$}-Renyi generalizations {\$}I{\_}{\{}\backslashalpha{\}}(A;B|C){\$} of the conditional mutual information that all converge to the conditional mutual information in the limit {\$}\backslashalpha \backslashto 1{\$}. Furthermore, we prove that many of these generalizations satisfy the aforementioned properties. As such, the quantities defined here should find applications in quantum information theory and perhaps even in other areas of physics, but we leave this for future work. Finally, we state a conjecture regarding the monotonicity of the Renyi conditional mutual informations defined here with respect to the Renyi parameter {\$}\backslashalpha{\$}. We prove that this conjecture is true in some special cases and provide evidence that it is true when {\$}\backslashalpha{\$} is in a neighborhood of one.},
  alphakey      = {BSW15},
  archiveprefix = {arXiv},
  arxivid       = {arXiv:1403.6102v2},
  author        = {Berta, Mario and Seshadreesan, Kaushik and Wilde, MM},
  doi           = {10.1063/1.4908102},
  eprint        = {arXiv:1403.6102v2},
  file          = {:Users/marcot/Documents/Mendeley Desktop/Berta, Seshadreesan, Wilde/2015/Berta, Seshadreesan, Wilde - R{\'{e}}nyi Generalizations of the Conditional Quantum Mutual Information - Journal of Mathematical Physics - 20.pdf:pdf},
  issn          = {0022-2488},
  journal       = {Journal of Mathematical Physics},
  link          = {http://arxiv.org/abs/1403.6102},
  number        = {2},
  pages         = {022205},
  title         = {R{\'{e}}nyi Generalizations of the Conditional Quantum Mutual Information},
