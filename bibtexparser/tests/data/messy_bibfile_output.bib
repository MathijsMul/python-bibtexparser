  eprint        = {1205.4182},
  file          = {:Users/marcot/Documents/Mendeley Desktop/Marin, Markham/2012/Marin, Markham - On the equivalence between sharing quantum and classical secrets, and error correction - Unknown - 2012.pdf:pdf},
  link          = {http://arxiv.org/abs/1205.4182},
