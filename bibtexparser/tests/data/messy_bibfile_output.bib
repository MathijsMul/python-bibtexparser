  issn          = {1751-8113},
  journal       = {Journal of Physics A: Mathematical and Theoretical},
  keyword       = {Quantum Physics},
  link          = {http://stacks.iop.org/1751-8121/44/i=9/a=095305?key=crossref.76328b6649641cf2029d4e2d705bc92b},
