  issn          = {1751-8113},
  journal       = {Journal of Physics A: Mathematical and Theoretical},
  keyword       = {Mathematical Physics,Quantum Physics},
  link          = {http://stacks.iop.org/1751-8121/46/i=5/a=055301?key=crossref.4974db37ce8f3ff706fe358f86c2591f},
