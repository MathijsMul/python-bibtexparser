  issn          = {0129-055X},
  journal       = {Reviews in Mathematical Physics},
  keyword       = {Information Theory,Mathematical Physics,Quantum Physics},
  link          = {http://arxiv.org/abs/1008.2529 http://www.worldscinet.com/rmp/23/2307/S0129055X11004412.html},
