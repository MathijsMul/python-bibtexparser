  file          = {:Users/marcot/Documents/Mendeley Desktop/Friedland, Gheorghiu, Gour/2013/Friedland, Gheorghiu, Gour - Universal Uncertainty Relations - Physical Review Letters - 2013.pdf:pdf},
  issn          = {0031-9007},
